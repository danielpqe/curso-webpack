{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "Curso de webpack",
  "main": "external/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --mode development",
    "build:local": "webpack --mode development",
    "build:external": "webpack --config ./external/webpack.config.js --mode development",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js --mode development",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js --mode development",
    "build:dev": "webpack --config ./webpack-dev-server/webpack.config.js --mode development --watch",
    "build:dev-server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --mode development",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js --mode development",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js --mode development",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js --mode development",
    "build:video": "webpack --config ./url-loader-video/webpack.config.js --mode development",
    "build:json": "webpack --config ./json-loader/webpack.config.js --mode development",
    "build:react": "webpack --config ./react/webpack.config.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielpqe/curso-webpack.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@danielpqe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielpqe/curso-webpack/issues"
  },
  "homepage": "https://github.com/danielpqe/curso-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^0.4.4",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "file-loader": "^2.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
