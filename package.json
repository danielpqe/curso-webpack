{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "Curso de webpack",
  "main": "external/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js",
    "build:dev": "webpack --config ./webpack-dev-server/webpack.config.js --watch",
    "build:dev-server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js ",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./url-loader-video/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js",
    "build:less": "webpack --config ./less-loader/webpack.config.js",
    "build:postcss": "webpack --config ./postcss-loader/webpack.config.js",
    "build:prevent": "webpack --config ./prevent-duplication/webpack.config.js",
    "build:vendor": "webpack --config ./vendor/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:dll:src": "webpack --config ./dll/webpack.config.js",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielpqe/curso-webpack.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@danielpqe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielpqe/curso-webpack/issues"
  },
  "homepage": "https://github.com/danielpqe/curso-webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "nib": "^1.1.2",
    "node-sass": "^4.9.4",
    "postcss": "^7.0.13",
    "postcss-loader": "^3.0.0",
    "rupture": "^0.7.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
